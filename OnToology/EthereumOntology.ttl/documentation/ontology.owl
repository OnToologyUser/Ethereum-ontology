    <!-- http://purl.org/dc/elements/1.1/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/def/EthereumOntology/hasABI -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/EthereumOntology/hasABI">
        <rdfs:domain rdf:resource="https://w3id.org/def/EthereumOntology/Transaction"/>
        <rdfs:range rdf:resource="https://w3id.org/def/SolidityABI/ABI"/>
        <dc:description>Specification for smart contract interactions on Ethereum</dc:description>
        <rdfs:label>has abi</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/EthereumOntology/hasAccessList -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/EthereumOntology/hasAccessList">
        <rdfs:domain rdf:resource="https://w3id.org/def/EthereumOntology/Transaction"/>
        <rdfs:range rdf:resource="https://w3id.org/def/EthereumOntology/AccessList"/>
        <dc:description>A list specifying addresses and keys accessed during the transaction, optimizing gas usage and enabling more predictable gas costs</dc:description>
        <rdfs:label>has access list</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/EthereumOntology/hasBehaviour -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/EthereumOntology/hasBehaviour">
        <rdfs:domain rdf:resource="https://w3id.org/def/EthereumOntology/Block"/>
        <rdfs:range rdf:resource="http://ethon.consensys.net/Block"/>
        <dc:description>Behaviour of the block represented in the EthOn ontology</dc:description>
        <rdfs:label>has behaviour</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/EthereumOntology/hasKeys -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/EthereumOntology/hasKeys">
        <rdfs:domain rdf:resource="https://w3id.org/def/EthereumOntology/AccessList"/>
        <rdfs:range rdf:resource="https://w3id.org/def/EthereumOntology/Keys"/>
        <dc:description>Specific storage locations accessed in a contract</dc:description>
        <rdfs:label>has keys</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/EthereumOntology/hasTransactions -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/EthereumOntology/hasTransactions">
        <rdfs:domain rdf:resource="https://w3id.org/def/EthereumOntology/Block"/>
        <rdfs:range rdf:resource="https://w3id.org/def/EthereumOntology/Transaction"/>
        <dc:description>Set of operations encoded and transmitted on the network</dc:description>
        <rdfs:label>has transactions</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/EthereumOntology/hasWithdrawals -->


    <owl:ObjectProperty rdf:about="https://w3id.org/def/EthereumOntology/hasWithdrawals">
        <rdfs:domain rdf:resource="https://w3id.org/def/EthereumOntology/Block"/>
        <rdfs:range rdf:resource="https://w3id.org/def/EthereumOntology/Withdrawals"/>
        <dc:description>Process of moving funds from a contract to an address</dc:description>
        <rdfs:label>has withdrawals</rdfs:label>
    </owl:ObjectProperty>
    


